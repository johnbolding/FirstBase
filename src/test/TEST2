2049,	# X: TEKTRONIX
2050,	#
2051,	tek|tek4012|4012|tektronix 4012:\
2052,		:do=^J:is=\E^O:le=^H:bs:cl=1000\E^L:co#75:ns:li#35:os:
2053,	tek4013|4013|tektronix 4013:\
2054,		:as=\E^N:ae=\E^O:dF#1000:tc=4012:
2055,	tek4014|4014|tektronix 4014:\
2056,		:is=\E^O\E9:co#81:li#38:dF#1000:tc=tek4012:
2057,	tek4015|4015|tektronix 4015:\
2058,		:as=\E^N:ae=\E^O:tc=4014:
2059,	tek4014-sm|4014-sm|tektronix 4014 in small font:\
2060,		:is=\E^O\E\072:co#121:li#58:tc=tek4014:
2061,	tek4015-sm|4015-sm|tektronix 4015 in small font:\
2062,		:as=\E^N:ae=\E^O:tc=4014-sm:
2063,	# I think the 1000UP is supposed to be so expensive it never happens.
