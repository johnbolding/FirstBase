# Makefile for Firstbase src/libwin
#

PRODUCT = @PRODUCT@
VERSION = @VERSION@

SHELL = /bin/sh
srcdir = @srcdir@
VPATH = @srcdir@

CC = @CC@
CFLAGS = @CFLAGS@
CPPFLAGS = @CPPFLAGS@
DEFS = @DEFS@
INSTALL = @INSTALL@
INSTALL_PROGRAM = @INSTALL_PROGRAM@
LDFLAGS = @LDFLAGS@
LIBS = @LIBS@ $(LIBEXTRA)

prefix = @prefix@
exec_prefix = @exec_prefix@
bindir = $(exec_prefix)/bin

COMPILE = $(CC) -c $(DEFS) $(INCLUDES) $(CPPFLAGS) $(CFLAGS)
LINK = $(CC) $(LDFLAGS) -o $@

.SUFFIXES:
.SUFFIXES: .c .o
.c.o:
	$(COMPILE) $< -o $@

INCLUDES = @INCLUDES@

OBJECTS=\
cx_com.o \
cx_excho.o

all: libwin
libwin: $(OBJECTS)

mostlyclean:
	rm -f *.o *._c *._o core core.*

clean: mostlyclean

distclean: clean
	rm -f Makefile

realclean: distclean
	rm -f TAGS

extraclean:: distclean
	rm -f patch* *~* *\#* *.orig *.rej *.bak core a.out

Makefile: Makefile.in ../../config.status
	cd ../.. && CONFIG_FILES=src/libwin/$@ CONFIG_HEADERS= ./config.status

names:
	@for name in $(OBJECTS) ; do \
	   echo libwin/$$name >> ../OBJECTS ; \
	done

MHEADERS= \
../include/fb.h  \
../include/config.h \
../include/input.h \
../include/error.h \
../include/message.h \
../include/libcc.h \
../include/libcdb.h \
../include/libdbd.h \
../include/libinit.h \
../include/liblic.h \
../include/librec.h \
../include/libscr.h \
../include/libwin.h \
../include/libsec.h \
../include/libedit.h \
../include/libclnt.h \
../include/interface.h \
../include/cx.h \
../include/fb_vars.h \
../include/fb_ext.h


cx_com.o:	$(MHEADERS)
cx_excho.o:	$(MHEADERS) ../include/vdict.h

DISTDIR = ../../$(PRODUCT)-$(VERSION)/src/libwin
DISTFILES = *.c Makefile.in

dist::
	@echo "Copying distribution files libwin"
	@for file in $(DISTFILES); do \
	   ln $(srcdir)/$$file $(DISTDIR) 2> /dev/null \
	      || cp -p $(srcdir)/$$file $(DISTDIR); \
	done

# Tell versions [3.59,3.63) of GNU make not to export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
